{
  "[markdown]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
  },
  "files.writable": true,
  "markdown.extension.toc.unorderedList.marker": "*",
  "java.compile.nullAnalysis.mode": "disabled",
}
